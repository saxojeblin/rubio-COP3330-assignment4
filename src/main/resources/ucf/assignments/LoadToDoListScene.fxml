<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="430.0" prefWidth="650.0" style="-fx-background-color: #D0D0D0;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.FXMLController">
   <children>
      <Button layoutX="89.0" layoutY="319.0" mnemonicParsing="false" onAction="#loadFileTextArea" prefHeight="65.0" prefWidth="130.0" style="-fx-background-color: #9D98F7; -fx-border-color: #000000;" text="Select File...">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextArea fx:id="loadFileTextArea" editable="false" focusTraversable="false" layoutX="48.0" layoutY="55.0" prefHeight="218.0" prefWidth="555.0" promptText="Please press 'Select File...' to load a file" style="-fx-border-color: #000000;">
         <font>
            <Font size="16.0" />
         </font>
      </TextArea>
      <Button layoutX="408.0" layoutY="319.0" mnemonicParsing="false" onAction="#submitLoadFileButton" prefHeight="65.0" prefWidth="130.0" style="-fx-background-color: #37B958; -fx-border-color: #000000;" text="Done">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Text layoutX="278.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Load File" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Label fx:id="loadFileErrorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="59.0" layoutY="287.0" prefHeight="17.0" prefWidth="533.0" textFill="RED">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Text layoutX="314.0" layoutY="412.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
